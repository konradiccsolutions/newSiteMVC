@model newSiteMVC.Models.tbl_UserControl

@if (!String.IsNullOrEmpty(Model.ImageUrl) && Model.PageId != "Home")
{
    <div id="intro" style="background-image: url(@Model.ImageUrl); background-position-y: 0px;">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                </div>
            </div>
        </div>
    </div>
}
else
{
    <div id="intro" style ="background-position-y: 0px !important;">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                </div>
            </div>
        </div>
    </div>
}
