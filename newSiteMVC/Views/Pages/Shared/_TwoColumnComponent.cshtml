@model IEnumerable<newSiteMVC.Models.tbl_UserControl>

<div class="container">
    <div class="row">
        @foreach (var item in Model.Where(it => it.TypeId == "TwoColumn"))
        {
        <div class="col-md-5 col-md-offset-1">
            <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="item active">
                        <img class="img-responsive img-hover crop-height-300 scale " style="box-shadow: 0px 0px 29px -12px;" src="/Img/training-cards.jpg" alt="">
                    </div>
                </div>
            </div>
        </div>

            <div class="col-md-4 col-md-offset-2">
                @Html.Raw(HttpUtility.HtmlDecode(item.MainText))                    
            </div>

            <div class="col-md-12 col-md-offset-7">
                @if (!String.IsNullOrEmpty(item.UrlLink))
                {
                <a class="btn btn-primary" href="@Html.DisplayFor(modelItem => item.UrlLink)" style="color:@Html.DisplayFor(modelItem => item.ButtonTextColour); background-color:@Html.DisplayFor(modelItem => item.ButtonColour)">@Html.DisplayFor(modelItem => item.ButtonText)</a>

                }
            </div>
         }

            </div>
    <div class="row" style="margin-top: -70px">
        <div class="col-lg-10 col-lg-offset-1">
            <h3 class="page-header">Our other products</h3>
        </div>

        <div class="col-sm-2 col-xs-6 col-sm-offset-1">
            <a href="#">
                <img class="img-responsive img-hover img-related crop-height-110" src="/Img/card-testing.jpg" alt="">
            </a>
        </div>

        <div class="col-sm-2 col-xs-6">
            <a href="#">
                <img class="img-responsive img-hover img-related crop-height-110" src="/Img/device-testing.jpg" alt="">
            </a>
        </div>

        <div class="col-sm-2 col-xs-6">
            <a href="#">
                <img class="img-responsive img-hover img-related crop-height-110" src="/Img/vantiv.jpg" alt="">
            </a>
        </div>

        <div class="col-sm-2 col-xs-6">
            <a href="#">
                <img class="img-responsive img-hover img-related crop-height-110" src="/Img/script-tools.png" alt="">
            </a>
        </div>
        <div class="col-sm-2 col-xs-6">
            <a href="#">
                <img class="img-responsive img-hover img-related crop-height-110" src="/Img/usacard1.jpg" alt="">
            </a>
        </div>

    </div>
</div>
